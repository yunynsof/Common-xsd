<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema xmlns="http://www.w3.org/2001/XMLSchema" targetNamespace="http://tigo.com.hn/scheduler/orders/TaskRequest/V1"
	xmlns:xsd="http://www.w3.org/2001/XMLSchema"
	xmlns:tns="http://tigo.com.hn/scheduler/orders/TaskRequest/V1"
	elementFormDefault="qualified">

  <xsd:complexType name="TaskRequest">
    <xsd:sequence>
      <xsd:element name="name" type="xsd:string" minOccurs="1"/>
      <xsd:element name="operationId" type="xsd:long" minOccurs="1"/>
      <xsd:element name="retry" type="xsd:int" minOccurs="1"/>
    </xsd:sequence>
  </xsd:complexType>
</xsd:schema>

